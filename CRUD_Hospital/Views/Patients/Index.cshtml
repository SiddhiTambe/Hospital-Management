@Model List<CRUD_Hospital.Models.Patient>

    <h1> Patients </h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Patient Name</th>
                <th>Address</th>
                <th>Mobile</th>
                <th>RegistrationDate</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var patient in Model)
            {
                <tr>
                    <td>@patient.Id</td>
                    <td>@patient.Name</td>
                    <td>@patient.Address</td>
                    <td>@patient.Mobile</td>
                    <td>@patient.RegistrationDate</td>
                    <td><a href ="Patients/View/@patient.Id">View</a></td>
                </tr>
            }
        </tbody>
    </table>